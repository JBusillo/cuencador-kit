		"@tailwindcss/jit": "^0.1.1",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"autoprefixer": "^10.2.5",
		"cssnano": "^4.1.10",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.1.0",
		"feather-icons": "4.25.0",
		"fs": "^0.0.1-security",
		"fs-extra": "^8.1.0",
		"google-auth-library": "^7.0.3",
		"node-fetch": "^2.6.1",
		"path": "^0.12.7",
		"postcss": "^8.2.7",
		"postcss-cli": "^8.3.1",
		"postcss-load-config": "^3.0.1",
		"prettier": "2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"sass": "^1.0.0",
		"showdown": "^1.9.1",
		"svelte": "^3.29.0",
		"svelte-hmr": "0.13.1",
		"svelte-preprocess": "^4.6.9",
		"tailwindcss": "^2.0.3",
